{
  "name": "jsonschema-form-electron-boilerplate",
  "version": "0.0.1",
  "description": "React-JSONschema-form Electron Boilerplate",
  "repository": "https://github.com/j-peterson/jsonschema-form-electron-boilerplate",
  "main": "src/index.js",
  "scripts": {
    "pack": "webpack --config webpack.config.js --progress --profile --colors",
    "start": "electron-forge start",
    "start-js": "electron src/main.js",
    "go": "npm run pack && npm run start-js",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "author": {
    "name": "Jeff Peterson",
    "email": "j.t.peterson01@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-jsonschema-form": "^0.43.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.64",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "electron": "^1.8.8",
    "webpack": "^2.2.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "jsonschema_form_electron_boilerplate"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
